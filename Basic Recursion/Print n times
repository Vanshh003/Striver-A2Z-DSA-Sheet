void solve(int n, vector<string> &ans) {
	if(n < 1) return;
	ans.push_back("Coding Ninjas");
	solve(n-1, ans);
}

vector<string> printNTimes(int n) {
	vector<string> ans;
	solve(n, ans);
	return ans;	
}
